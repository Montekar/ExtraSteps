<AnchorPane prefHeight="574.0" prefWidth="572.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.MainController">
   <children>
      <MenuBar prefHeight="26.0" prefWidth="228.0">
        <menus>
          <Menu mnemonicParsing="false" text="Movie">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#addMovie" text="Add" />
                  <MenuItem mnemonicParsing="false" onAction="#editMovie" text="Edit" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteMovie" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Category">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#addCategory" text="Add" />
                  <MenuItem mnemonicParsing="false" onAction="#editCategory" text="Edit" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteCategory" text="Delete" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Top">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#addMovie" text="Recommended " />
                  <MenuItem mnemonicParsing="false" onAction="#editMovie" text="All the time" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteMovie" text="Action" />
               </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Extras ">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#goTo" text="Go to IMDB" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="movieTable" layoutX="-1.0" layoutY="159.0" prefHeight="295.0" prefWidth="572.0">
        <columns>
          <TableColumn fx:id="colMovieTitle" prefWidth="304.80003052949905" text="Title" />
          <TableColumn fx:id="colMovieYear" prefWidth="131.199951171875" text="Year" />
            <TableColumn fx:id="colMovieRating" prefWidth="136.79994201660156" text="Rating" />
        </columns>
      </TableView>
      <ChoiceBox fx:id="choiceCategory" layoutX="416.0" layoutY="13.0" prefHeight="26.0" prefWidth="147.0" />
      <TextField fx:id="searchBar" alignment="CENTER" layoutY="132.0" prefHeight="26.0" prefWidth="578.0" promptText="Search For Movies" />
<!--      <Rating fx:id="movieRating" layoutX="108.0" layoutY="510.0" max="10" prefHeight="18.0" prefWidth="216.0" rating="0.0" />-->
      <Text layoutX="237.0" layoutY="501.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rating:">
         <font>
            <Font size="32.0" />
         </font>
      </Text>
      <Text layoutX="347.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category: ">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button fx:id="playMovie" layoutX="464.0" layoutY="497.0" mnemonicParsing="false" onAction="#playMovie" text="Play" />
   </children>
